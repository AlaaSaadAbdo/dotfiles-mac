  "explorer.sources": [
    {
      "name": "buffer",
      "expand": false
    },
    {
      "name": "file",
      "expand": true
    }
  ],
  "explorer.keyMappings.global": {
    "<space>": "toggleSelection",
    "C": "cd",
   "<cr>": ["expandable?", ["expanded?", "collapse", "expand"], "open"]
  },
  "explorer.presets": {
    "floating": {
      "position": "floating"
   }
