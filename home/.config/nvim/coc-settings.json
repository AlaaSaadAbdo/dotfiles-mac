{
  "codeLens.enable": true,
  "typescript.referencesCodeLens.enable": true,
  "typescript.implementationsCodeLens.enable": true,
  "suggest.maxPreviewWidth": 100,
  "suggest.enablePreselect": false,
  "diagnostic.checkCurrentLine": true,
  "diagnostic.maxWindowHeight": 20,
  "git.addGBlameToBufferVar": true,
  "git.addGBlameToVirtualText": true,
  "explorer.icon.enableVimDevicons": false,
  "explorer.previewAction.onHover": false,
  "explorer.file.autoReveal": true,
  "git.enableGutters": true,
  "python.jediEnabled": false,
  "explorer.file.showHiddenFiles": false,
  "explorer.sources": [
    {
      "name": "buffer",
      "expand": false
    },
    {
      "name": "file",
      "expand": true
    }
  ],
  "explorer.keyMappings": {
    "<space>": "toggleSelection",
    "C": "cd",
   "<cr>": ["expandable?", ["expanded?", "collapse", "expand"], "open"]
  },
  "explorer.presets": {
    "floating": {
      "position": "floating"
   }
  },
  "explorer.icon.enableNerdfont": true,
  "explorer.file.column.git.showIgnored": false,
  "git.addedSign.hlGroup": "GitGutterAdd",
  "git.changedSign.hlGroup": "GitGutterChange",
  "git.removedSign.hlGroup": "GitGutterDelete",
  "git.topRemovedSign.hlGroup": "GitGutterDelete",
  "git.changeRemovedSign.hlGroup": "GitGutterChangeDelete",
  "list.normalMappings": {
  "t": "action:tabe",
  "E": "action:vsplit",
  "s": "action:split",
  "d": "expr:GetDeleteAction"
  },
    "languageserver": {
    "terraform": {
      "command": "terraform-ls",
      "args":["serve"],
      "filetypes": ["terraform"],
      "initializationOptions": {}
    },
    "bash": {
      "command": "bash-language-server",
      "args": ["start"],
      "filetypes": ["sh"],
      "ignoredRootPaths": ["~"]
    }
  },
  "tsserver.npm": "/Users/amansour/.asdf/shims/npm",
  // "tsserver.log": "verbose",
  // "tsserver.trace.server": "verbose",
  "snippets.ultisnips.directories": [
  "UltiSnips",
  "gosnippets/UltiSnips"
  ],
  "eslint.autoFixOnSave": true,
  "eslint.filetypes": ["javascript", "javascriptreact", "typescript", "typescriptreact"],
  "coc.preferences.formatOnTypeFiletypes": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
    "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "coc.preferences.formatOnInsertLeave": true,
  "tsserver.formatOnType": true,
  "coc.preferences.formatOnType": true,
  "eslint.packageManager":"yarn",
  "go.goplsPath": "/Users/amansour/go/bin/gopls",
  "go.goplsOptions": {
    "completeUnimported": true
  }

}
